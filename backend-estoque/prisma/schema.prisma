generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id             String  @id @default(uuid())
  name           String
  description    String?
  image          String?
  type           String
  expirationDate String? @map("expiration-date")
  gender         String?
  size           Int?

  @@map("products")
}
